<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/onboardingFragment">
           
    <fragment
        android:id="@+id/onboardingFragment"
        android:name="br.com.compass.compassmart.ui.fragment.tela_onboarding.OnboardingFragment"
        android:label="fragment_onboarding"
        tools:layout="@layout/fragment_onboarding">
        <action
            android:id="@+id/action_onboardingFragment_to_produtosFragment"
            app:destination="@id/produtosFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="br.com.compass.compassmart.ui.fragment.tela_login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_cadastroFragment"
            app:destination="@id/cadastroFragment" />
        <action
            android:id="@+id/action_loginFragment_to_carrinhoComprasFragment"
            app:destination="@id/carrinhoComprasFragment" />
    </fragment>
    <fragment
        android:id="@+id/carrinhoComprasFragment"
        android:name="br.com.compass.compassmart.ui.fragment.tela_carrinhocompras.CarrinhoComprasFragment"
        android:label="fragment_carrinho_compras"
        tools:layout="@layout/fragment_carrinho_compras">
        <action
            android:id="@+id/action_carrinhoComprasFragment_to_enderecoFragment"
            app:destination="@id/enderecoFragment" />
    </fragment>
    <fragment
        android:id="@+id/meioPagamento"
        android:name="br.com.compass.compassmart.ui.fragment.tela_meiopagamento.MeioPagamento"
        android:label="fragment_meio_pagamento"
        tools:layout="@layout/fragment_meio_pagamento" >
        <action
            android:id="@+id/action_meioPagamento_to_parabenizacaoFragment"
            app:destination="@id/parabenizacaoFragment" />
    </fragment>
    <fragment
        android:id="@+id/produtosFragment"
        android:name="br.com.compass.compassmart.ui.fragment.tela_produtos.ProdutosFragment"
        android:label="fragment_produtos"
        tools:layout="@layout/fragment_produtos" >
        <action
    android:id="@+id/action_produtosFragment_to_detalhesDoProdutoFragment"
    app:destination="@id/detalhesDoProdutoFragment" />
</fragment>
    <fragment
        android:id="@+id/detalhesDoProdutoFragment"
        android:name="br.com.compass.compassmart.ui.fragment.tela_detalhes.DetalhesDoProdutoFragment"
        android:label="fragment_detalhes_do_produto"
        tools:layout="@layout/fragment_detalhes_do_produto" >
        <action
            android:id="@+id/action_detalhesDoProdutoFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <argument
            android:name="produto"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/enderecoFragment"
        android:name="br.com.compass.compassmart.ui.fragment.tela_endereco.EnderecoFragment"
        android:label="fragment_endereco"
        tools:layout="@layout/fragment_endereco" >
        <action
            android:id="@+id/action_enderecoFragment_to_confirmacaoFragment"
            app:destination="@id/confirmacaoFragment" />
    </fragment>
    <fragment
        android:id="@+id/cadastroFragment"
        android:name="br.com.compass.compassmart.ui.fragment.tela_cadastro.CadastroFragment"
        android:label="fragment_cadastro"
        tools:layout="@layout/fragment_cadastro" >
        <action
            android:id="@+id/action_cadastroFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/parabenizacaoFragment"
        android:name="br.com.compass.compassmart.ui.fragment.tela_parabenizacao.ParabenizacaoFragment"
        android:label="fragment_parabenizacao"
        tools:layout="@layout/fragment_parabenizacao">
    </fragment>
    <fragment
        android:id="@+id/confirmacaoFragment"
        android:name="br.com.compass.compassmart.ui.fragment.tela_confirmacao.ConfirmacaoFragment"
        android:label="fragment_confirmacao"
        tools:layout="@layout/fragment_confirmacao" >
        <action
            android:id="@+id/action_confirmacaoFragment_to_meioPagamento"
            app:destination="@id/meioPagamento" />
    </fragment>
    <fragment
        android:id="@+id/favoritosFragment"
        android:name="br.com.compass.compassmart.ui.fragment.tela_produtos.FavoritosFragment"
        android:label="FavoritosFragment" >
        <action
            android:id="@+id/action_favoritosFragment_to_detalhesDoProdutoFragment"
            app:destination="@id/detalhesDoProdutoFragment" />
    </fragment>
</navigation>